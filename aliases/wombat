discord_install() {
  cd ~/Downloads
  DISCORD_INSTALL_FILE="$(ls | grep discord)"
  sudo dpkg -i $DISCORD_INSTALL_FILE
  rm $DISCORD_INSTALL_FILE
  cd -
}